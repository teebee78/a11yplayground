    "@angular/platform-browser": "^21.1.1",
    "@angular/router": "^21.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "^21.1.1",
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/compiler-cli": "^21.1.1",
    "@cypress/schematic": "^4.1.4",
    "@types/jasmine": "~5.1.0",
    "axe-core": "^4.11.0",
    "cypress": "15.7.0",
    "cypress-axe": "^1.7.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "pa11y": "^9.0.1",
    "pa11y-ci": "^4.0.1",
